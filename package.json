{
  "name": "@gnosis.pm/multicall",
  "version": "1.0.2",
  "description": "MakerDAO multicall contracts",
  "license": "MIT",
  "keywords": [
    "multicall",
    "smart contracts",
    "ethereum",
    "gnosis"
  ],
  "scripts": {
    "lint": "npx solhint  \"contracts/**/*.sol\"",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "flatten": "npx hardhat flatten ./contracts/Multicall.sol > ./flatten/Multicall.sol && npx hardhat flatten ./contracts/Multicall2.sol > ./flatten/Multicall2.sol",
    "deploy": "npx hardhat deploy --network",
    "sourcify": "npx hardhat sourcify --network"
  },
  "dependencies": {
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@truffle/contract": "^4.2.23",
    "dotenv": "^10.0.0",
    "hardhat": "^2.4.1",
    "hardhat-deploy": "^0.8.11",
    "hardhat-gas-reporter": "^1.0.4",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.16",
    "truffle-assertions": "^0.9.2",
    "web3": "^1.4.0",
    "web3-utils": "1.2.6"
  },
  "engines": {
    "node": "=14.X"
  },
  "devDependencies": {
    "hardhat-abi-exporter": "^2.2.1"
  }
}
